{
    "name": "Librus-cleaner",
    "description": "your mother will never beat you again",
    "version": "2.3",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://synergia.librus.pl/przegladaj_nb/*"],
            "js":  ["content.js"]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://synergia.librus.pl/*"
    ]
}